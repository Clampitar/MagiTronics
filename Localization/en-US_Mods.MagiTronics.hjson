	UsageTerminal: {
		DisplayName: Usage Terminal
		Tooltip:
			'''
			Recieves item usage from redirectors
			Can be removed with wire cutters
			'''
	}

	Redirector: {
		DisplayName: Redirector
		Tooltip:
			'''
			Redirects item usage towards tiles with terminals
			Terminals must be wired
			'''
	}

	LogicBuffer: {
		DisplayName: Logic Filter
		Tooltip:
			'''
			Place between logic gates and logic lamps to change their behaviour
			The filter is the input and the gate is the output
			'''
	}

	TickTimer: {
		DisplayName: Tick Timer
		Tooltip:
			'''
			Activates every tick
			Logic lamps can be placed over it to determine the number of ticks between activations
			'''
	}

	SignalCounter: {
		DisplayName: Signal Counter
		Tooltip:
			'''
			Counts signals in binary
			Logic lamps placed above it will send wire signals
			'''
	}

	BossSensor: {
		DisplayName: Logic Sensor (Danger)
		Tooltip: Activates when there is danger
	}

	LonelySensor: {
		DisplayName: Logic sensor (Lonely)
		Tooltip: Activates When it is seperate from any tiles or walls
	}

	ItemUsor: {
		DisplayName: Item Usor
		Tooltip:
			'''
			Uses items
			Activated by wire
			'''
	}

	AutoPicker: {
		DisplayName: Auto Picker
		Tooltip:
			'''
			Not a crafting station
			70% pickaxe power
			Mines tiles automatically
			'''
	}
